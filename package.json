{
   "name": "grass",
   "version": "0.0.1",
   "private": false,
   "license": "MIT",
   "description": "API nodejs with typescript",
   "devDependencies": {
      "@types/graphql": "^14.5.0",
      "@types/node": "^8.0.29",
      "nodemon": "^2.0.7",
      "ts-node": "3.3.0",
      "tsc-watch": "^4.2.8",
      "tsconfig-paths": "^3.9.0",
      "typescript": "3.9.6"
   },
   "dependencies": {
      "@hapi/joi": "^17.1.1",
      "agora-access-token": "^2.0.4",
      "apollo-server": "^2.25.1",
      "apollo-server-express": "^2.15.1",
      "aws-sdk": "^2.928.0",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-validator": "^0.12.2",
      "commander": "^4.1.1",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.15.4",
      "firebase": "^7.15.5",
      "fs-extra": "^9.0.0",
      "graphql": "^15.2.0",
      "graphql-subscriptions": "^1.1.0",
      "graphql-upload": "^11.0.0",
      "is-pdf": "^1.0.0",
      "is-svg": "^4.2.1",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.19",
      "module-alias": "^2.2.2",
      "multer": "^1.4.3",
      "multer-s3": "^2.9.0",
      "mysql": "^2.14.1",
      "mysql2": "^2.1.0",
      "nexmo": "^2.9.1",
      "nodemailer": "^6.4.2",
      "reflect-metadata": "^0.1.10",
      "sequelize": "^6.6.2",
      "sequelize-cli": "^5.5.1",
      "subscriptions-transport-ws": "^0.9.18",
      "type-graphql": "^1.1.1",
      "typeorm": "0.2.22"
   },
   "_moduleAliases": {
      "@entity": "build/app/Entity",
      "@resolver": "build/app/Resolvers",
      "@input": "build/app/Input",
      "@util": "build/app/Utilities",
      "@middleware": "build/app/Middleware",
      "@notification": "build/app/Notifications",
      "@namespace": "build/app/Namespace",
      "@schema": "build/app/Schema",
      "@router": "build/routes",
      "@controller": "build/controller",
      "@repository": "build/app/Repositories",
      "@service": "build/app/Services"
   },
   "scripts": {
      "dev": "tsc-watch --onSuccess \"nodemon ./build/index.js\"",
      "build:prod": "node --max-old-space-size=8192 node_modules/typescript/bin/tsc",
      "build": "tsc",
      "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "make:migration": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
      "recreatedb": "ts-node ./node_modules/typeorm/cli.js schema:drop && npm run migrate",
      "rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert",
      "command": "ts-node command",
      "seed": "./node_modules/.bin/sequelize db:seed:all"
   }
}
