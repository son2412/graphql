{
   "name": "grass",
   "version": "0.0.1",
   "private": false,
   "license": "MIT",
   "description": "API nodejs with typescript",
   "devDependencies": {
      "@types/graphql": "^14.5.0",
      "@types/node": "^8.0.29",
      "nodemon": "^1.18.10",
      "ts-node": "3.3.0",
      "tsc-watch": "^4.2.8",
      "tsconfig-paths": "^3.9.0",
      "typescript": "3.9.6"
   },
   "dependencies": {
      "apollo-server": "^2.15.1",
      "apollo-server-express": "^2.15.1",
      "aws-sdk": "^2.613.0",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-validator": "^0.12.2",
      "commander": "^4.1.1",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.15.4",
      "firebase": "^7.15.5",
      "fs-extra": "^9.0.0",
      "graphql": "^15.2.0",
      "graphql-subscriptions": "^1.1.0",
      "graphql-upload": "^11.0.0",
      "is-pdf": "^1.0.0",
      "is-svg": "^4.2.1",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.15",
      "module-alias": "^2.2.2",
      "multer": "^1.4.2",
      "multer-s3": "^2.9.0",
      "mysql": "^2.14.1",
      "mysql2": "^2.1.0",
      "nexmo": "^2.6.0",
      "nodemailer": "^6.4.2",
      "passport": "^0.4.1",
      "passport-facebook": "^3.0.0",
      "reflect-metadata": "^0.1.10",
      "sequelize": "^5.21.5",
      "sequelize-cli": "^5.5.1",
      "socket.io": "^2.3.0",
      "subscriptions-transport-ws": "^0.9.18",
      "type-graphql": "^1.0.0-rc.3",
      "typeorm": "0.2.22"
   },
   "_moduleAliases": {
      "@entity": "build/app/Entity",
      "@resolver": "build/app/Resolvers",
      "@input": "build/app/Input",
      "@service": "build/app/Services",
      "@middleware": "build/app/Middleware",
      "@notification": "build/app/Notifications",
      "@namespace": "build/app/Namespace"
   },
   "scripts": {
      "start": "tsc && nodemon --watch ./src --exec ts-node -r tsconfig-paths/register ./src/index.ts",
      "dev": "tsc-watch --onSuccess \"nodemon ./build/index.js\"",
      "prod": "rm -rf build && tsc && nodemon build/index.js",
      "build": "tsc",
      "ts": "nodemon src/index.ts",
      "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "make:migration": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
      "recreatedb": "ts-node ./node_modules/typeorm/cli.js schema:drop && npm run migrate",
      "rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert",
      "command": "ts-node command",
      "seed": "./node_modules/.bin/sequelize db:seed:all"
   }
}
